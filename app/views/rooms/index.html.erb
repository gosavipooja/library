	<h1>List of rooms</h1>
 
<body>
<table style="width:100%">
  <tr>
    <th>Room number</th>
    <th>Size</th>
	<th>Building</th>
  </tr>
  
 
 	
  <% @rooms.each do |room| %>
    <tr>
      <td><%= room.room_no %></td>
      <td><%= room.size %></td>
	  <td><%= room.building %></td>
      <td><%= link_to 'Show', room_path(room) %></td>
	  <td><%= link_to 'Edit', edit_room_path(room) %></td>
	  <td><%= link_to 'Delete', room_path(room),
              method: :delete,
              data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
  <tr>
  </tr>
  <tr>
  <%= link_to 'Create', new_room_path  %>
  </tr>
  
</table>
</body>

